<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, maximum-scale=1"
    />
    <!-- ========== Title ========== -->
    <title>CSR Portal : Event Page</title>
    <!-- ========== Favicon Ico ========== -->
    <!--<link rel="icon" href="fav.ico">-->
    <!-- ========== STYLESHEETS ========== -->
    <!-- Bootstrap CSS -->
    <link href="view-assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Fonts Icon CSS -->
    <link href="view-assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="view-assets/css/et-line.css" rel="stylesheet" />
    <link href="view-assets/css/ionicons.min.css" rel="stylesheet" />
    <!-- Carousel CSS -->
    <link href="view-assets/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="view-assets/css/owl.theme.default.min.css" rel="stylesheet" />
    <!-- Animate CSS -->
    <link rel="stylesheet" href="view-assets/css/animate.min.css" />
    <!-- Custom styles for this template -->
    <link href="view-assets/css/main.css" rel="stylesheet" />
  </head>
  <body>
    <div class="loader">
      <div class="loader-outter"></div>
      <div class="loader-inner"></div>
    </div>

    <!--header start here -->
    <header class="header navbar fixed-top navbar-expand-md">
      <div class="container">
        <a class="navbar-brand logo" href="#">
          <img src="view-assets/img/satya.png" alt="Evento" />
          <span class="logo-span" ><b>CSR<br> Portal<b></span>
        </a>
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#headernav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="lnr lnr-text-align-right"></span>
        </button>
        <div
          class="collapse navbar-collapse flex-sm-row-reverse"
          id="headernav"
        >
          <ul class=" nav navbar-nav menu">
            <li class="nav-item">
              <a class="nav-link active" href="/userIndex">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link " href="#">Organizers</a>
            </li>
            <li class="nav-item">
              <a class="nav-link " href="#">Events</a>
            </li>
            <li class="nav-item">
              <a class="nav-link " href="#">News</a>
            </li>
            <li class="nav-item">
              <a class="nav-link " href="#">Contact</a>
            </li>
            <li><a href="http://csrportal.forumotion.com" target="_blank">Forum</a>
            </li>
          </ul>
        </div>
      </div>
    </header>
    <!--header end here-->

    <!-- -->
    <!--cover section slider -->
    <section id="home" class="home-cover">
      <div class="cover_slider owl-carousel owl-theme">
        <div
          class="cover_item"
          style="background: url('view-assets/img/bg/slider.jpg');"
        >
          <div class="slider_content">
            <div class="slider-content-inner">
              <div class="container">
                <div class="slider-content-center">
                  <h2 class="cover-title">
                    Prepare yourself for the
                  </h2>
                  <strong class="cover-xl-text">Biggest Workshop</strong>
                  <p class="cover-date">
                    12-14 February 2019 - Delhi, India
                  </p>
                  <a href="#" class=" btn btn-primary btn-rounded">
                    Contribute
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="cover_nav">
        <ul class="cover_dots">
          <li class="active" data="0"><span>1</span></li>
          <li data="1"><span>2</span></li>
          <li data="2"><span>3</span></li>
        </ul>
      </div>
    </section>
    <!--cover section slider end -->

    <!--event info -->
    <section class="pt100 pb100">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-6 col-md-3  ">
            <div class="icon_box_two">
              <i class="ion-ios-calendar-outline"></i>
              <div class="content">
                <h5 class="box_title">
                  DATE
                </h5>
                <p>
                  12-14 february 2019
                </p>
              </div>
            </div>
          </div>

          <div class="col-6 col-md-3  ">
            <div class="icon_box_two">
              <i class="ion-ios-location-outline"></i>
              <div class="content">
                <h5 class="box_title">
                  location
                </h5>
                <p>
                  Delhi, India
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--event info end -->

    <!--event countdown -->
    <section
      class="bg-img pt70 pb70"
      style="background-image: url('view-assets/img/bg/bg-img.jpg');"
    >
      <div class="overlay_dark"></div>
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-12 col-md-10">
            <h4 class="mb30 text-center color-light">
              Counter until the event
            </h4>
            <div class="countdown"></div>
          </div>
        </div>
      </div>
    </section>
    <!--event count down end-->

    <!--about the event -->
    <section class="pt100 pb100">
      <div class="container">
        <div class="section_title">
          <h3 class="title">
            About the event
          </h3>
        </div>
        <div class="row justify-content-center">
          <div class="col-12 col-md-6">
            <p>
              Describes the events
            </p>
          </div>
          <div class="col-12 col-md-6">
            <p>
              
            </p>
          </div>
        </div>

        <!--event features-->
        <div class="row justify-content-center mt30">
          <div class="col-12 col-md-6 col-lg-3">
            <div class="icon_box_one">
              <i class="lnr lnr-mic"></i>
              <div class="content">
                <h4>Fund</h4>
                <p>
                  Approx 2Cr.
                </p>
                <a href="#">read more</a>
              </div>
            </div>
          </div>

          <div class="col-12 col-md-6 col-lg-3">
            <div class="icon_box_one">
              <i class="lnr lnr-rocket"></i>
              <div class="content">
                <h4>Man Power</h4>
                <p>
                  Approx 10 people
                </p>
                <a href="#">read more</a>
              </div>
            </div>
          </div>

          <div class="col-12 col-md-6 col-lg-3">
            <div class="icon_box_one">
              <i class="lnr lnr-bullhorn"></i>
              <div class="content">
                <h4>Service Provider</h4>
                <p>
                  Book and accomodation
                </p>
                <a href="#">read more</a>
              </div>
            </div>
          </div>

        </div>
        <!--event features end-->
      </div>
    </section>
    <!--about the event end -->

   
    <div class="copyright_footer">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-md-6 col-12">
            <p>
              Created in Smart India Hackathon 2019
            </p>
          </div>
          <div class="col-12 col-md-6 ">
            <ul class="footer_menu">
              <li>
                <a href="#">Events</a>
              </li>
              <li>
                <a href="#">News</a>
              </li>
              <li>
                <a href="#">Contact</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <!--footer end -->

    <!-- jquery -->

    <script src="view-assets/js/jquery.min.js"></script>
    <!-- bootstrap -->
    <script src="view-assets/js/popper.js"></script>
    <script src="view-assets/js/bootstrap.min.js"></script>
    <script src="view-assets/js/waypoints.min.js"></script>
    <!--slick carousel -->
    <script src="view-assets/js/owl.carousel.min.js"></script>
    <!--parallax -->
    <script src="view-assets/js/parallax.min.js"></script>
    <!--Counter up -->
    <script src="view-assets/js/jquery.counterup.min.js"></script>
    <!--Counter down -->
    <script src="view-assets/js/jquery.countdown.min.js"></script>
    <!-- WOW JS -->
    <script src="view-assets/js/wow.min.js"></script>
    <!-- Custom js -->
    <script src="view-assets/js/main.js"></script>
